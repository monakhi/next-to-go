{
  "name": "next-to-go",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx,.vue",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest run"
  },
  "lint-staged": {
    "*.{ts,tsx,vue,js,jsx}": [
      "eslint --fix"
    ],
    "*.{ts,tsx,vue}": [
      "vue-tsc --noEmit -p tsconfig.json --skipLibCheck false --pretty false"
    ]
  },
  "dependencies": {
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.14",
    "@tailwindcss/vite": "^4.1.14",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.13",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "~10.4.0",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.3",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.14",
    "typescript": "~5.9.0",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.0"
  }
}
